{
    "name": "fake34526/chaster-factory",
    "description": "Chaster Factory",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "fake34526/chaster-objects": "^0.4",
        "zenstruck/foundry": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "rector/rector": "^1.0",
        "symfony/test-pack": "^1.0.9"
    },
    "authors": [
        {
            "name": "Fake",
            "email": "fake@fake.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5 | 9.5.8 | >=10"
    },
    "autoload": {
        "psr-4": {
            "Fake\\ChasterFactory\\": "src/",
            "Fake\\ChasterFactory\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/yrmltyrxnwtzsjlfvz/chaster-dto-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/yrmltyrxnwtzsjlfvz/chaster-objects.git"
        }
    ]
}
